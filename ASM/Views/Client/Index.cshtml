 <div class="main-content">
     <div class="container-fluid">
         <div class="row">
             <div class="col-lg-9 col-md-8 col-sm-12 col-xs-12 pull-right collection-content-right">
                 <div class="content-product-list">
                     @foreach (var item in Model)
                     {
                         <div class="product-loop">
                             <div class="product-block product-resize"
                                  data-value="coke-zero-330ml">
                                 <div class="product-img"
                                      data-link="/products/coke-zero-330ml">
                                     <a href="/Product/Details/@item.Id" title="@item.Name" class="image-resize ratiobox">
                                         <picture>
                                             <img class="lazyload img-loop"
                                                  data-sizes="auto"
                                                  data-src="https://product.hstatic.net/200000291373/product/5_d2293e415c6142949f9dc6eaee0bbf6f_dc28a94958c145e2ac484bb6662d0c15_5dd6cfd760674f8ea8c435ded66d8c28_grande.png"
                                                  data-lowsrc="https://product.hstatic.net/200000291373/product/5_d2293e415c6142949f9dc6eaee0bbf6f_dc28a94958c145e2ac484bb6662d0c15_5dd6cfd760674f8ea8c435ded66d8c28_grande.png"
                                                  src="@Html.DisplayFor(modelItem => item.Image)"
                                                  alt=" COKE ZERO 330ML " />
                                         </picture>
                                         <a href="/Product/Details/@item.Id" title="@item.Name">@item.Name</a>
                                     </a>
                                 </div>
                                 <div class="product-detail">
                                     <h3 class="product-name">
                                         <div class="product-action">
                                             <div class="box-product-prices">
                                                 <div class="product-price">
                                                     <span>@Html.DisplayFor(modelItem => item.Price)</span>
                                                 </div>
                                             </div>
                                             <div class="button-add">
                                                 <button type="submit"
                                                         class="action pro-btn-order"
                                                         data-link="/products/coke-zero-330ml">
                                                     Đặt món
                                                 </button>
                                             </div>
                                         </div>
                                     </h3>

                                 </div>
                             </div>
                         </div>
                     }
                 </div>
                 <div class="sortpagibar pagi clearfix text-center">
                     <div id="pagination" class="clearfix">
                         <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                             <span class="page-node current">1</span>

                             <a class="page-node" href="all4658.html?page=2">2</a>

                             <a class="page-node" href="all9ba9.html?page=3">3</a>

